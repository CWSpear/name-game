{
  "name": "name-game",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng:start": "ng serve",
    "ng:build": "ng build",
    "ng:test": "ng test",
    "srv:start": "nodemon --watch 'server/**/*.ts' --ignore 'server/**/*.spec.ts' --exec 'ts-node' server/index.ts",
    "srv:build": "tsc --project server",
    "build": "npm run srv:build && npm run ng:build",
    "test": "npm run ng:test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "node dist/server/index",
    "push": "docker-compose -f docker-compose.yml -f docker-compose.production.yml build && docker-compose -f docker-compose.yml -f docker-compose.production.yml push",
    "deploy": "docker-compose -f docker-compose.yml -f docker-compose.production.yml pull && docker-compose -f docker-compose.yml -f docker-compose.production.yml up -d"
  },
  "private": true,
  "dependencies": {
